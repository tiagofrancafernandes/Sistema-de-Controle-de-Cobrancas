{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./resources/js/*"
            ],
            "@resources/*": [
                "./resources/*"
            ],
            "@public/*": [
                "./public/*"
            ],
            "@asset/*": [
                "./public/*"
            ],
            "@components/*": [
                "./resources/js/Components/*"
            ],
            "@CRUD/*": [
                "./resources/js/Pages/CRUD/*"
            ],
            "@SvgIcons/*": [
                "./resources/js/SvgIcons/*"
            ],
            "@EasyCrud/*": [
                "./resources/js/EasyCrud/*"
            ],
            "ziggy-js": [
                "./vendor/tightenco/ziggy"
            ]
        },
    },
    "exclude": [
        "node_modules",
        "public"
    ],
    "include": [
        "./src/**/*",
        "env.d.ts",
        "./resources/js/**/*",
        "./resources/js/**/*.js",
        "resources/js/**/*.js",
        "./resources/js/**/*.{js,vue,ts}"
    ],
}
